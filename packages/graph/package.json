{
  "name": "@repo/graph",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/**/*.ts",
      "!**/*.test.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "bundle": false,
    "minify": false,
    "clean": true,
    "format": [
      "esm"
    ],
    "dts": true
  },
  "exports": {
    "./graphRunner": {
      "types": "./dist/graphRunner.d.ts",
      "default": "./dist/graphRunner.js"
    },
    "./sampleCollector": {
      "types": "./dist/schemas/sampleCollector.d.ts",
      "default": "./dist/schemas/sampleCollector.js"
    },
    ".": {
      "types": "./dist/configuration.d.ts",
      "default": "./dist/configuration.js"
    }
  },
  "scripts": {
    "start": "node --loader ts-node/esm ./src/bin/start.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "test": "vitest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@playwright/test": "^1.44.0",
    "lodash": "catalog:",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "catalog:",
    "@types/node": "catalog:",
    "ts-node": "^10.9.1",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "^1.5.2"
  }
}

{
  "name": "snapflow",
  "version": "0.0.15",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "my-start": "./dist/graph/bin/start.js"
  },
  "exports": {
    ".": "./dist/graph/index.js"
  },
  "scripts": {
    "build": "pnpm --filter \"snapflow-graph\" --filter \"snapflow-reporter\" run build && sh build.sh",
    "build:graph": "pnpm --filter \"snapflow-graph\" run build",
    "build:reporter": "pnpm --filter \"snapflow-reporter\" run build",
    "serve": "pnpm --filter \"snapflow-reporter\" run serve"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}

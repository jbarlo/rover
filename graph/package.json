{
  "name": "snapflow",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "format": [
      "esm"
    ],
    "dts": true,
    "minify": true
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "start": "node --loader ts-node/esm ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && tsup",
    "test": "vitest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "ts-node": "^10.9.1",
    "tsup": "^8.1.0",
    "typescript": "^5.1.6",
    "vitest": "^1.5.2"
  }
}
